{
  "DetailedErrors": true,
  "ConnectionStrings": {
    "AppContext": "Data Source=../MyTemplate.Data/Database.db;"
  },
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://*:4433"
      },
      "Http": {
        "Url": "http://*:6000"
      }
    }
  },
  "SupportedCulturesOptions": {
    "SupportedCultures": [
      {
        "Display": "Nederlands",
        "Culture": "nl-BE"
      },
      {
        "Display": "English",
        "Culture": "en-US"
      }
    ]
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File",
      "Serilog.Sinks.Seq",
      "Serilog.Enrichers.Environment"

    ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Information"

      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:HH:mm:ss} [{Level:u3}] ({MachineName}) : {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      },
      {
        "Name": "File",
        "Args": {
          "outputTemplate": "{Timestamp:HH:mm:ss} [{Level:u3}] ({MachineName}) : {Message}{NewLine}{Exception}",
          "path": "./logs/log.log",
          "rollOnFileSizeLimit": true,
          "fileSizeLimitBytes": 547483648,
          "retainedFileCountLimit": 5
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ]

  }
}
